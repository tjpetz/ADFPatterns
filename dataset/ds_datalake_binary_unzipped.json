{
	"name": "ds_datalake_binary_unzipped",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_pattern_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"layer": {
				"type": "string",
				"defaultValue": "bronze"
			},
			"payer": {
				"type": "string",
				"defaultValue": "humana"
			},
			"importTimestamp": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().payer, '/', dataset().importTimestamp)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().layer",
					"type": "Expression"
				}
			}
		}
	}
}